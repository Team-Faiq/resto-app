<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffc480"
    tools:context=".DilaActivityCabangLokasi">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/croissant_one"
                android:gravity="center"
                android:text="PIlihan Cabang Resto"
                android:textColor="@android:color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/bt_yogyakarta"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_yogyakarta"
                android:text="YOGYAKARTA"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_surabaya" />

            <Button
                android:id="@+id/bt_surabaya"
                android:layout_width="170sp"
                android:layout_height="50sp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_surabaya"
                android:text="SURABAYA"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_bogor" />

            <Button
                android:id="@+id/bt_jakarta"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_jakarta"
                android:text="Jakarta"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <Button
                android:id="@+id/bt_bogor"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_bogor"
                android:text="BOGOR"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_jakarta" />


            <Button
                android:id="@+id/bt_semarang"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_semarang"
                android:text="SEMARANG"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_yogyakarta" />

            <Button
                android:id="@+id/bt_malang"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_malang"
                android:text="MALANG"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_semarang" />

            <Button
                android:id="@+id/bt_solo"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_solo"
                android:text="SOLO"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_malang" />

            <Button
                android:id="@+id/bt_purwokerto"
                android:layout_width="170dp"
                android:layout_height="50dp"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="70dp"
                android:background="#5C3A11"
                android:fontFamily="@font/bangers"
                android:onClick="alamat_purwokerto"
                android:text="PURWOKERTO"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bt_solo" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
